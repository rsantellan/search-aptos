<form method="post" action="{{ path('apartamento_delete', {'url': apartamento.url}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ apartamento.url) }}">
    <button class="btn">Delete</button>
</form>
